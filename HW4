from datetime import datetime
dates_input = ['Wednesday, October 2, 2002','Friday, 11.10.13','Thursday, 18 August 1977']
def date_parce(dates, DATE_FORMATS = ['%A, %B %d, %Y','%A, %d.%m.%y','%A, %d %B %Y']):
  for date in dates:
    for format in DATE_FORMATS:
      try:
        dt = datetime.strptime(date, format)
        print(f"Тип: {type(dt)}, значение: {dt}")
      except ValueError:
        continue
date_parce(dates_input)
